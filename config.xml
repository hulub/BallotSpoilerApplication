<?xml version='1.0' encoding='utf-8'?>
<widget id="com.aion.ballotspoiler" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>BallotSpoiler</name>
    <description>
        A blank PhoneGap app.
    </description>
    <author email="support@phonegap.com" href="http://phonegap.com">
        PhoneGap Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <engine name="browser" spec="~5.0.4" />
    <!-- add a version here, otherwise PGB will use whatever the latest version of the package on npm is -->
    <plugin name="phonegap-plugin-barcodescanner" version="8.0.0" />
    <preference name="android-build-tool" value="gradle" />
</widget>
